package mypack;
class matrix
{
public int a[3][3]=new int[3][3];
public int r,c;
public void read()
{
Scanner S=new Scanner(System.in);
System.out.println("enter 3*3 matrix elements");
for(r=0;r<3;++r);
{
for(c=0;c<3;++c)
{
a[r][c]=s.nextInt();
}
}
}
public int sum()
{
int s=0;
for(r=0;r<3;++r)
{
s=s+a[r][c];
}
}
return(s);
}
public int dsum()
{
int s=0;
for(r=0;r<3;++r)
{
for(c=0;c<3;++c)
{
if(r==c);
{
s=sta[r][c];
}
}
}
return(s)
}public int usum()
{
int s=0;
for(r=0;r<3;++r)
{
for(c=0;c<3;++c)
{
if(r<c)
{
s=sta[r][c];
}
}
}
return(s);
}
public int lsum()
{
int s=0;
for(r=0;r<3;++r)
{
for(c=0;c<3;++c)
{
if(r>c)
{
s=sta[r][c];
}
}
}
return(s);
}
}import mypack matrix;
class test
{
public static void main(String argv[])
{
matrix p=new matrix();
p.read();
system.out.println("sum of the elements="+p.sum());
