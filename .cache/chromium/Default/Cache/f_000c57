    <script language="javascript" type="text/javascript">
        function SearchSite() {
            if (document.getElementById('q').value == "" || document.getElementById('q').value.length == 0) {
                return false;
            } else {
                document.getElementById('btnsearch').focus();
                document.location.href = 'http://kvsangathan.nic.in/SearchResult.aspx?cx=009588453489510557425%3Aguggkoiviw0&cof=FORID%3A9&ie=UTF-8&q=' + document.getElementById('q').value + '&sa=Search';
            }
        }

    </script>
    <script language="javascript" type="text/javascript">
    var INIT_SZ, MAX, MIN;

var init = function() {
    INIT_SZ = 13;
    MAX = 15;
    MIN = 11;  
    var elem = document.getElementsByTagName("body");
    elem[0].style.fontSize = 13;
}

var enlarge = function() { 
var elem = document.getElementsByTagName("body");
var size = parseInt(elem[0].style.fontSize.replace("px", ""));
if(size < MAX)
 {  
  elem[0].style.fontSize = (size + 1) + "px";       
 }
}   
  
var shrink = function(){  
var elem = document.getElementsByTagName("body");   
var size = parseInt(elem[0].style.fontSize.replace("px", "")); 
if(size > MIN) {  
   elem[0].style.fontSize = (size - 1) + "px"; 
   }    
}
     </script>

