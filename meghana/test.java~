import java.util.*;
import p1.cube;
import p1.square;
class test
{
public static void main(String args[])
{
int x;
System.out.print("enter a number");
Scanner s=new Scanner(System.in);
x=s.nextInt();
cube c=new cube();
square a=new square();
System.out.print("cube="+(c.cube(x)));
System.out.print("square="+(a.square(x)));
}
}

